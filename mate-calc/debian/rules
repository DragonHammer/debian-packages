#!/usr/bin/make -f

%:
	dh $@

override_dh_install:
	dh_install --fail-missing

override_dh_auto_configure:
	NOCONFIGURE=1 ./autogen.sh
	dh_auto_configure -- \
		--disable-static \
		--disable-silent-rules

override_dh_strip:
	dh_strip --dbg-package=mate-calc-dbg

get-orig-source:
	uscan --noconf --force-download --rename --download-current-version --destdir=..
