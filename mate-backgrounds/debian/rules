#!/usr/bin/make -f

DHFLAGS=--parallel

%:
	dh $@ $(DHFLAGS)

override_dh_auto_configure:
	NOCONFIGURE=1 ./autogen.sh
	dh_auto_configure $(DHFLAGS) -- \
		--disable-maintainer-mode

override_dh_install:
	dh_install --list-missing

get-orig-source:
	uscan --noconf --force-download --rename --download-current-version --destdir=..
